        android:label="@string/stars"
        android:roundIcon="@mipmap/ic_launcher8_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.ElysiumRise"
        tools:targetApi="31">
